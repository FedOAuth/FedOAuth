#: fedoauth/views.py:40
msgid "You have been logged out"
msgstr "Wylogowano"

#: fedoauth/views_openid.py:177
msgid "None provided"
msgstr "Nie podano"

#: fedoauth/auth/fas.py:176
msgid "Incorrect username or password"
msgstr "Niepoprawna nazwa użytkownika lub hasło"

#: fedoauth/auth/fas.py:184
#, python-format
msgid "This service is limited to the following users: %(users)s"
msgstr "Ta usługa jest ograniczona do następujących użytkowników: %(users)s"

#: fedoauth/templates/auth_fas_login.html:2
#: fedoauth/templates/auth_fas_login.html:17
msgid "Login"
msgstr "Login"

#: fedoauth/templates/auth_fas_login.html:4
#, python-format
msgid ""
"%(trust_root)s is asking to authenticate using the Fedora Account System"
msgstr "%(trust_root)s prosi o uwierzytelnienie używając Systemu kont Fedory"

#: fedoauth/templates/auth_fas_login.html:7
msgid "Login with your FAS credentials"
msgstr "Zaloguj za pomocą danych uwierzytelniających FAS"

#: fedoauth/templates/auth_fas_login.html:11
msgid "FAS Username"
msgstr "Nazwa użytkownika FAS"

#: fedoauth/templates/auth_fas_login.html:14
msgid "Password"
msgstr "Hasło"

#: fedoauth/templates/index.html:2
msgid "Home"
msgstr "Strona domowa"

#: fedoauth/templates/index.html:4
msgid "This is the Federated Open Authentication provider homepage."
msgstr "Strona domowa dostawcy rozproszonego, otwartego systemu uwierzytelniania."

#: fedoauth/templates/index.html:7
msgid ""
"You will be redirected to this application whenever another application "
"requires you to authenticate."
msgstr "Użytkownik zostanie przekierowany do tej aplikacji, kiedy inna aplikacja wymaga uwierzytelnienia."

#: fedoauth/templates/openid_user.html:2
msgid "Userpage"
msgstr "Strona użytkownika"

#: fedoauth/templates/openid_user.html:4
#, python-format
msgid "This is the OpenID page for %(username)s."
msgstr "To jest strona OpenID dla użytkownika %(username)s."

#: fedoauth/templates/openid_user.html:7
msgid "This page is primarily used internally"
msgstr "Ta strona jest używana wewnętrznie"

#: fedoauth/templates/openid_user_ask_trust_root.html:2
msgid "Approval"
msgstr "Akceptacja"

#: fedoauth/templates/openid_user_ask_trust_root.html:4
#, python-format
msgid ""
"%(trust_root)s is asking to authenticate via OpenID using the Fedora Account"
" System"
msgstr "%(trust_root)s prosi o uwierzytelnienie przez OpenID używając Systemu kont Fedory"

#: fedoauth/templates/openid_user_ask_trust_root.html:8
msgid "Review the authorization details"
msgstr "Przejrzyj szczegóły upoważnienia"

#: fedoauth/templates/openid_user_ask_trust_root.html:20
msgid "Privacy Policy"
msgstr "Polityka prywatności"

#: fedoauth/templates/openid_user_ask_trust_root.html:31
msgid "Group"
msgstr "Grupa"

#: fedoauth/templates/openid_user_ask_trust_root.html:37
msgid "FPCA Completed"
msgstr "Ukończono FPCA"

#: fedoauth/templates/openid_user_ask_trust_root.html:38
msgid "True"
msgstr "Prawda"

#: fedoauth/templates/openid_user_ask_trust_root.html:44
msgid "Approve"
msgstr "Zaakceptuj"

#: fedoauth/templates/openid_user_ask_trust_root.html:47
msgid "Reject"
msgstr "Odrzuć"
